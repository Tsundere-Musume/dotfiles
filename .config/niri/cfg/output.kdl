output "eDP-2" {
    mode "1920x1080@144"
    variable-refresh-rate
    scale 1
    transform "normal"
    position x=0 y=1080
    hot-corners {
        off
    }
}
output "HDMI-A-1" {
    mode "1920x1080@60"
    scale 1
    transform "normal"
    position x=0 y=0
    hot-corners {
        off
    }
}
